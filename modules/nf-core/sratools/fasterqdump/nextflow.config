process {
    withName: SRATOOLS_FASTERQDUMP {
        ext.args = '--split-files --include-technical'
        publishDir = [
            path: { "${params.outdir}/public/${meta.study_accession}/${meta.library_strategy}/fastq" },
            mode: params.publish_dir_mode,
            pattern: "*.fastq.gz"
        ]
    }
}