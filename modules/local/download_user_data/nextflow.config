process {
    withName: 'DOWNLOAD_USER_DATA' {
        publishDir = [
            [
                path: { "${params.outdir}/internal/${meta.study_accession}/${meta.library_strategy}/fastq" },
                mode: params.publish_dir_mode,
                pattern: "*.fastq.gz"
            ],
            [
                path: { "${params.outdir}/internal/${meta.study_accession}/${meta.library_strategy}/fastq/md5" },
                mode: params.publish_dir_mode,
                pattern: "*.md5"
            ]
        ]
    }
}
